doctype html
html
  head
    title Add a Custom Object
  body
    h1 Add a Custom Object
    form(action="/update-coi" method="POST")
      label(for="name") Name:
      input(type="text" name="name" required)
      br

      label(for="colour") Colour:
      input(type="text" name="colour" required)
      br

      label(for="size") Size:
      input(type="text" name="size" required)
      br

      button(type="submit") Submit
